<script>

(function() {
	
	
	

	var log_prefix = '🐶 ';
	var ms = 2500;

	
	var data = {
		storage_key: 'reload_attempts',
		reload_count: 0,
		reload_count_max: 1,
	};

	function warn(msg) {
		
		if (window.console && console.warn) return console.warn(log_prefix + msg);
	}

	function log(msg) {
		
		if (window.console && console.log) return console.log(log_prefix + msg);
	}

	function showTroubleLoading() {
		if (document.body) {
			document.body.className += ' has_loading_trouble';
			window.setTimeout(function() {
				document.body.className += ' loading_trouble_transition';
			}, 100);
			beaconFailure('client_load_trouble');
		} else {
			warn('showTroubleLoading: WTF could not find document.body!?');
		}
	}

	function beaconFailure(label) {
		
		if (!label) return;
		if (window.TSBeacon) {
			window.TSBeacon(label, 1);
		} else {
			log('No window.TSBeacon to log failure "' + label + '", boo.');
		}
	}

	function checkCSS() {
		
		
		
		var i;
		var j;
		var countable_stylesheets = 0;
		var domain = 'slack-edge.com';
		var stylesheets_with_rules = 0;
		var rules;
		var found_rules;
		var sheet;
		var sheet_rule_count;
		var bad_css = [];
		var has_boot_data = window.TS && TS.boot_data;
		var is_dev = has_boot_data && TS.boot_data.version_ts && TS.boot_data.version_ts === 'dev';

		
		
		if (!has_boot_data) {
			warn('CSS check: TS and/or TS.boot_data are not defined. Exiting.');
			return;
		}

		if (!document.styleSheets || !document.styleSheets.length) {
			warn('CSS check: No styleSheets present? Exiting.');
			return;
		}

		for (i = 0, j = document.styleSheets.length; i < j; i++) {
			sheet = document.styleSheets[i];
			rules = sheet && (sheet.cssRules || sheet.rules);
			sheet_rule_count = 0;

			
			
			
			if (!sheet.href) continue;

			
			if (sheet.href && sheet.href.indexOf(domain) === -1 && !is_dev) {
				continue;
			}

			
			if (!rules) {
				warn('CSS check: no cssRules or rules defined?', sheet);
				continue;
			}

			countable_stylesheets += 1;

			
			
			
			found_rules = true;

			sheet_rule_count = Object.keys(rules).length;

			
			
			stylesheets_with_rules += sheet_rule_count ? 1 : 0;

			
			if (!sheet_rule_count) bad_css.push(sheet.href);
		}

		sheet = null;
		rules = null;

		
		if (found_rules) {
			var css_ok = found_rules && stylesheets_with_rules >= countable_stylesheets;
			
			if (!css_ok) {
				warn('CSS check: Some CSS failed to load or had no rules.\n' + bad_css.join('\n'));
				beaconFailure('client_load_trouble_some_css_missing');
			}
		} else {
			warn(
				'CSS check: No CSS rules found. Counted ' +
					countable_stylesheets +
					', found ' +
					stylesheets_with_rules +
					' with rules, ' +
					bad_css.length +
					' without ' +
					(bad_css.length ? '\n' + bad_css.join('\n') : '')
			);
			beaconFailure('client_load_trouble_all_css_missing');
		}
	}

	
	
	var storage = {
		get: function() {
			var key = data.storage_key;
			try {
				return window.sessionStorage && sessionStorage.getItem(key);
			} catch (err) {
				warn('unable to get sessionStorage key "' + key + '".');
			}
		},
		set: function(value) {
			var key = data.storage_key;
			try {
				return window.sessionStorage && sessionStorage.setItem(key, value);
			} catch (err) {
				warn('unable to set sessionStorage key "' + key + ' = ' + value + '".');
			}
		},
		remove: function() {
			var key = data.storage_key;
			try {
				return window.sessionStorage && sessionStorage.removeItem(key);
			} catch (err) {
				warn('unable to remove sessionStorage key "' + key + '".');
			}
		},
	};

	function countFailure() {
		
		data.reload_count = parseInt(storage.get(), 10) || 0;
		
		data.reload_count += 1;
		storage.set(data.reload_count);
		if (data.reload_count > data.reload_count_max) {
			
			warn(
				'No TS or !TS.client after ' +
					ms +
					' msec - giving up, showing load trouble screen.'
			);
			storage.remove();
			data.reload_count = 0;
			showTroubleLoading();
			return;
		}
		warn(
			'No TS or !TS.client after ' +
				ms +
				' msec - reloading (attempt ' +
				data.reload_count +
				'/' +
				data.reload_count_max +
				')'
		);
		
		return true;
	}

	function fallbackReload(no_cache) {
		
		window.location.reload(no_cache);
	}

	function checkJS() {
		if (!window.TS || !TS.client) {
			warn(
				'CDN JS watchdog: No TS or TS.client in page? Possible client / network / CDN failure. May reload in ' +
					ms +
					' ms.'
			);
			window.setTimeout(function() {
				if (!window.TS || !TS.client) {
					var needs_reload = countFailure();
					if (needs_reload) {
						var no_cache = true;
						
						
						if (window.URL) {
							var url = new URL(window.location.href);
							
							if (url.searchParams) {
								url.searchParams.set('cdn_fallback', data.reload_count);
								window.location.href = url.href;
							} else {
								fallbackReload(no_cache);
							}
						} else {
							fallbackReload(no_cache);
						}
						return;
					}
				}

				log(
					'TS and TS.client recovered after inline check failed, loaded within ' +
						ms +
						' ms.'
				);

				
				if (data.reload_count) {
					log(
						'Loaded OK after reload_count = ' +
							data.reload_count +
							'. Clearing count from sessionStorage.'
					);
					storage.remove();
				}
			}, ms);
		}
	}

	
	if (window.envIsBeingRebuilt) return;

	
	checkJS();

	
	window.setTimeout(checkCSS, ms);
})();
</script>


